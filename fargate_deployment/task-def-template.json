{
  "family": "REPLACE_TASK_FAMILY",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "REPLACE_EXEC_ROLE",
  "taskRoleArn": "REPLACE_TASK_ROLE",
  "containerDefinitions": [
    {
      "name": "diabetes",
      "image": "REPLACE_IMAGE_URI",
      "essential": true,
      "environment": [
        {"name": "AWS_REGION", "value": "REPLACE_AWS_REGION"},
        {"name": "BUCKET", "value": "REPLACE_BUCKET"},
        {"name": "PREFIX", "value": "REPLACE_PREFIX"},
        {"name": "SELECTED_FEATURES_FILE", "value": "REPLACE_SELECTED_FEATURES_FILE"},
        {"name": "FILTERED_INPUT_FILE", "value": "REPLACE_FILTERED_INPUT_FILE"},
        {"name": "XGB_OUTPUT_PREFIX", "value": "REPLACE_XGB_OUTPUT_PREFIX"},
        {"name": "SAGEMAKER_TRAINING_ROLE", "value": "REPLACE_SAGEMAKER_TRAINING_ROLE"}
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "REPLACE_LOG_GROUP",
          "awslogs-region": "REPLACE_AWS_REGION",
          "awslogs-stream-prefix": "REPLACE_LOG_STREAM_PREFIX"
        }
      }
    }
  ]
}
